.navbar {
  width: 100%;
  background-color: #f2f4dc;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
}
.title-container {
  height: 44px;
  width: 100%;
  display: flex;
  align-items: center;
  position: relative;
  justify-content: center;
}
.nav-back {
  position: absolute;
  left: 10px;
  font-family: "webfont";
}
.nav-back-icon {
  height: 20px;
  width: 20px;
  transition: all 0.2s ease-in-out;
}
.nav-back-icon:focus {
  transform: scale(2);
}
.title {
  color: #927f6e;
  font-weight: bolder;
  font-size: 14px;
  flex-grow: 1;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-family: "webfont";
}
