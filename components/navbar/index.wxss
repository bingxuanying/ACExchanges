.navbar {
  width: 100%;
  background-color: #fbf6e0;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
}
.title-container {
  height: 44px;
  width: 100%;
  display: flex;
  align-items: center;
  position: relative;
  justify-content: center;
}
.nav-back {
  position: absolute;
  left: 0px;
  font-family: "webfont";
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  padding: 0 10px;
}
.nav-home {
  position: absolute;
  left: 30px;
  font-family: "webfont";
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  padding: 0 10px;
}
.nav-back-icon {
  height: 20px;
  width: 20px;
  transition: all 0.2s ease-in-out;
  margin-left: 5px;
}
.nav-back-icon:focus {
  transform: scale(2);
}
.title {
  color: #927f6e;
  font-weight: bolder;
  font-size: 14px;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-family: "webfont";
}
